<?import javafx.scene.control.ComboBox?>

<StackPane styleClass="command-pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <ComboBox fx:id="commandComboBox" onAction="#handleCommandInputChanged" />
    <StackPane.margin>
      <Insets left="10.0" />
    </StackPane.margin></TextField>
  <ImageView id="keyboardImage" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER_RIGHT">
